@model Database.Entities.Lab

<div class="model fade" id="addLab">
	<div class="model-dialog">
		<div class="modal-content">
			<div class="model-header">
				<h4 class="modal-title" id="addLabLabel">Add Lab</h4>
				<button type="button" class="close" data-dismiss="model">
					<span></span>
				</button>
			</div>
			<div class="modal-body">
				<form action="Create">
					<div class="form-group">
						<label asp-for="@Model.Name"></label>
						<input asp-for="@Model.Name" class="form-control" placeholder="Name"/>
						<span asp-validation-for="@Model.Name" class="text-danger"></span>
					</div>
					<div class="form-group">
						<label asp-for="@Model.PhoneNumber"></label>
						<input asp-for="@Model.PhoneNumber" class="form-control" placeholder="Phone Number" />
						<span asp-validation-for="@Model.PhoneNumber" class="text-danger"></span>
					</div>
				</form>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-secondary" data-dismiss="model">Close</button>
				<button type="button" class="btn btn-primary" data-save="model">Save</button>
			</div>
		</div>
	</div>
</div>